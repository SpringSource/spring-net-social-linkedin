@model IEnumerable<Spring.Social.LinkedIn.Api.Certification>

<table>
    <tr>
        <th>
            Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
      
    </tr>
}

</table>
